
@charset 'utf-8';
.featuredbg {
	background:#11600d url(../images/skins/sleek/green/background.jpg) repeat-x center top;
	}
	
.featuredtotal {
	background:#11600d;;
	}

.featuredband {
	background:url(../images/skins/sleek/green/bottomfeature.png) center bottom no-repeat;
	}

.featuredbg h1, .featuredbg h2 {
	color:#ffffff;
	text-shadow:#000000 0px -1px 1px;;
 }
	
.featuredbg h3, .featuredbg h4 {
  	color: #90e590;
 	text-shadow:#000000 0px -1px 1px;;
 	padding-top:0;
}

.latest h4 a:hover, .latest h5 a:hover, .sidebar h5 a:hover, 
.sidebar h4 a:hover, h5 a:hover, h4 a:hover {
	text-decoration:none;
	color: #165316;
}

.image-desc p, .image-desc ul, .image-desc ol, .featuredbg p {
color:#90e590;
text-shadow:#000 0px -1px 0px;
}

ul.thumbs img, ul.thumbs img a, ul.thumbs img a:visited, ul.thumbs img a:active {
	border:#ffffff 3px solid;
}

.dark.button{
	background-color: #011301;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#165316', endColorstr='#011301'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#165316), to(#011301)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #165316,  #011301); /* for firefox 3.6+ */ 
	border:#011301 1px solid;
	text-shadow:#000000 0 -1px 0;
	color:#ffffff;
}

.dark.button:hover {
	background-color:#90e590;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#90e590', endColorstr='#58bc58'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#90e590), to(#58bc58)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #90e590,  #58bc58); /* for firefox 3.6+ */ 
	color: #000000;
	text-shadow: 0 1px 0px #ffffff;
	border:#58bc58 1px solid;
}	
.light.button {
	background-color:#90e590;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#90e590', endColorstr='#58bc58'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#90e590), to(#58bc58)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #90e590,  #58bc58); /* for firefox 3.6+ */ 
	color: #000000;
	text-shadow: 0 1px 0px #ffffff;
	border:#58bc58 1px solid;
}
.light.button:hover {
	color: #ffffff;
	background: #011301;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#222222', endColorstr='#000000'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#222222), to(#000000)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #222222,  #000000); /* for firefox 3.6+ */ 
	border:#000000 1px solid;
	text-shadow:#000000 0 -1px 0;
}
.testimonial p {
	color:#ffffff;
}

a {
	color:#165316;
}

.footer a {
	color:#58bc58;
}


