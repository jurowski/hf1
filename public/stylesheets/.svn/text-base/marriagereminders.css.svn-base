
a {
	text-decoration: underline;
	color: #516C00;
}


body {
background-color: #D7DFE2;
margin:0px;
padding:0px;
font: 16px  verdana, arial, helvetica;
color: #416271;
}
h1{
padding:5px 0 5px 0;
margin:0px;
font-size:24px;
color:#FFFFFF;
}
h2{
padding:5px 0 5px 0;
margin:0px;
font-size:24px;
color:#f7951e;
}
h3{
padding:0 0 5px 0;
margin:0px;
font-size:18px;
color:#59cef9;
}
h4{
padding:0 0 10px 0;
margin:0px;
font-size:18px;
color:#7ea743;
}
p{
text-align:justify;
padding:2px 0 2px 0;
margin:0px;
line-height:15px;
}
p.white{
color:#FFFFFF;
}
p.green{
color:#d4f0ba;
}
p.clear{
clear:both;
padding:10px 0 0 0;
}
img.left_img{
border:1px #cae6c6 solid;
float:left;
margin:0 10px 0 0 ;
}
img.box_img{
float:left;
padding:10px 20px 0 0 ;
}
#main_container{
width:900px;
height:auto;
margin:auto;
background-color:#FFFFFF;
}
#header{
width:900px;
height:auto;
margin:auto;
}
#logo{
padding:15px 0 0 30px;
float:left;
}
/*----------------------menu-------------------------*/
#menu{
width:900px;
height:42px;
padding:0px;
clear:both;
background-color:#416271;
}
#menu ul{
list-style:none;padding:0 0 0 230px;margin: auto;display:block;
}
#menu ul li{
list-style:none;display:inline;float:left;width:auto;height:42px;padding:0px;line-height:42px; font-family:Arial, Helvetica, sans-serif; font-size:13px;
}
#menu ul li a{
height:42px; width: auto;float:left;text-decoration:none;padding:0 0 0 25px; margin:0px 3px 0px 3px;color:#fff;text-align:center;
}
#menu ul li a:hover{
height:42px;width: auto;text-decoration:none;color: #fff;padding:0 0 0 25px; background:url(/images/marriagereminders//menu_arrow.gif) no-repeat left;
}
#menu ul li a.current{
height:42px;width: auto;float:left;text-decoration:none;padding:0px;color: #fff;padding:0 0 0 25px; background:url(/images/marriagereminders//menu_arrow.gif) no-repeat left;
}

/*----------------middle green box-----------*/
.green_box{
width:900px;
height:210px;
clear:both;
background:url(/images/marriagereminders//green_box_bg.gif) no-repeat left #50a842;
background-position:50px 0px;
}
.clock{
float:left;
padding:0px;
}
.text_content{
width:665px;
float:left;
padding:20px 0 0 0;
}
.read_more{
width:101px;
height:29px;
float:right;
padding:10px;
background:url(/images/marriagereminders//read_more_bg.gif) no-repeat center;
line-height:29px;
text-align:center;
}
.read_more a{
text-decoration:none;
color:#FFFFFF;
}
.read_more_link{
width: auto;
float:right;
padding:3px 25px 3px 0;
background:url(/images/marriagereminders//orange_bullet.gif) no-repeat right;
}
.read_more_link a{
text-decoration:none;
color:#f7941d;
}
/*----------------------left navigation-------------------------*/
#left_nav{
width:680px;height: auto;padding:5px 0 0 0; float:left;
}
#left_nav ul{
list-style:none;margin: auto;display:block; padding:0px;
}
#left_nav ul li{
list-style:none;display:block;width:auto;height:29px;padding:0px;line-height:29px;
}
#left_nav ul li a{
height:29px;display:block; width:680px;text-decoration:none;padding:0 0 0 35px; margin:7px 0 7px 0;color:#000000;background:url(/images/marriagereminders//left_nav_arrow.gif) no-repeat left; border-bottom:0px #bedf77 dashed;
background-position:0px 0px;
font-size:16px;
font-family:Arial, Helvetica, sans-serif;

}
/*--------------------------main_content-------------------*/
#main_content{
width:auto;
height:auto;
clear:both;
}
#left_content{
width:320px;
height:auto;
margin:10px;
float:left;
padding:10px;
background:url(/images/marriagereminders//left_content_bg.gif) repeat-x top;
}
#right_content{
width:850px;
height:auto;
margin:0;
float:left;
padding:0 0 20px 20px;
}
/*----------------------top right navigation-------------------------*/
#right_nav{
width:263px;height: auto;padding:20px 0 0 50px; float:left;
}
#right_nav ul{
list-style:none;margin: auto;display:block; padding:0px;
}
#right_nav ul li{
list-style:none;display:block;width:auto;height:29px;padding:0px;line-height:29px;
}
#right_nav ul li a{
height:29px;display:block; width:263px;text-decoration:none;padding:0 0 0 35px; margin:7px 0 7px 0;color:#d4f0ba;background:url(/images/marriagereminders//right_nav.gif) no-repeat center;
background-position:0px 0px;
}
#right_nav ul li a:hover{
height:29px;display:block;width:263px;text-decoration:none;color: #d4f0ba;padding:0 0 0 35px;margin:7px 0 7px 0; background:url(/images/marriagereminders//right_nav_a.gif) no-repeat center;
background-position:0px 0px;
}
#right_nav ul li a.current{
height:29px;display:block;width:263px;text-decoration:none;padding:0px;color: #d4f0ba;padding:0 0 0 35px;margin:7px 0 7px 0; 
background:url(/images/marriagereminders//right_nav_a.gif) no-repeat center;
background-position:0px 0px;
}

.products_box{
height:210px;
padding:10px 0 5px 0;
border-bottom:1px #a7ef6e dashed;
}
.search_box{
width:267px;
height:124px;
float:left;
margin:15px 10px 10px 0;
background:url(/images/marriagereminders//search_box_bg.gif) no-repeat center;
