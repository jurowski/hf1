/* Footer */

#footer {
	background-color: #eae6cd;
	height: 30px;
	margin: 0 auto;
	padding: 0px 0 15px 0;
	font-family: Arial, Helvetica, sans-serif;
	width: 960px;
}

#footer p {
	font-weight:bold;
	margin: 0;
	line-height: normal;
	font-size: 12px;
	text-align: center;
	padding-top: 15px;
	color: #000000;
}

#footer a {
	font-weight:bold;
	text-decoration: none;
	color: #000000;
}
