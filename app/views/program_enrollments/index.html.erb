<h1>Listing program_enrollments</h1>

<table>
  <tr>
    <th>Program</th>
    <th>User</th>
    <th>Active</th>
    <th>Ongoing</th>
    <th>Start date</th>
    <th>End date</th>
  </tr>

<% @program_enrollments.each do |program_enrollment| %>
  <tr>
    <td><%=h program_enrollment.program_id %></td>
    <td><%=h program_enrollment.user_id %></td>
    <td><%=h program_enrollment.active %></td>
    <td><%=h program_enrollment.ongoing %></td>
    <td><%=h program_enrollment.start_date %></td>
    <td><%=h program_enrollment.end_date %></td>
    <td><%= link_to 'Show', program_enrollment %></td>
    <td><%= link_to 'Edit', edit_program_enrollment_path(program_enrollment) %></td>
    <td><%= link_to 'Destroy', program_enrollment, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New program_enrollment', new_program_enrollment_path %>