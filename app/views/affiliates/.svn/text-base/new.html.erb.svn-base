<h1>New affiliate</h1>

<% form_for(@affiliate) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </p>
  <p>
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </p>
  <p>
    <%= f.label :affiliate_name %><br />
    <%= f.text_field :affiliate_name %>
  </p>
  <p>
    <%= f.label :coupon_code %><br />
    <%= f.text_field :coupon_code %>
  </p>
  <p>
    <%= f.label :coupon_discount %><br />
    <%= f.text_field :coupon_discount %>
  </p>
  <p>
    <%= f.label :affiliate_earnings_rate %><br />
    <%= f.text_field :affiliate_earnings_rate %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', affiliates_path %>