<h1>New message_goal</h1>

<% form_for(@message_goal) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :message_id %><br />
    <%= f.text_field :message_id %>
  </p>
  <p>
    <%= f.label :goal_id %><br />
    <%= f.text_field :goal_id %>
  </p>
  <p>
    <%= f.label :trigger_id %><br />
    <%= f.text_field :trigger_id %>
  </p>
  <p>
    <%= f.label :sent_date %><br />
    <%= f.date_select :sent_date %>
  </p>
  <p>
    <%= f.label :sent_datetime %><br />
    <%= f.datetime_select :sent_datetime %>
  </p>
  <p>
    <%= f.label :sent_status %><br />
    <%= f.text_field :sent_status %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_area :notes %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', message_goals_path %>