<h1>My teams</h1>

<table>
  <tr>
    <th>Team Name</th>
    <th>Category</th>
    <th>Members</th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= link_to team.name, team %></td>
    <td><%=h team.category_name %></td>
    <td><%=h team.qty_current %>/<%=h team.qty_max %></td>
  </tr>
<% end %>
</table>

<br />

