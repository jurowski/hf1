<h1>New program</h1>

<% form_for(@program) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :organization_id %><br />
    <%= f.text_field :organization_id %>
  </p>
  <p>
    <%= f.label :managed_by_user_id %><br />
    <%= f.text_field :managed_by_user_id %>
  </p>
  <p>
    <%= f.label :image_logo %><br />
    <%= f.text_field :image_logo %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :about %><br />
    <%= f.text_area :about %>
  </p>
  <p>
    <%= f.label :joined_count %><br />
    <%= f.text_area :joined_count %>
  </p>
  <p>
    <%= f.label :current_count %><br />
    <%= f.text_area :current_count %>
  </p>
  <p>
    <%= f.label :rating_stars %><br />
    <%= f.text_field :rating_stars %>
  </p>
  <p>
    <%= f.label :rating_votes %><br />
    <%= f.text_field :rating_votes %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', programs_path %>