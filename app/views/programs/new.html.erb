<h1>New program</h1>

<% form_for(@program) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :about %><br />
    <%= f.text_area :about %>
  </p>

    <h3>You can add tags once the template is saved.</h3>



  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', programs_path %>