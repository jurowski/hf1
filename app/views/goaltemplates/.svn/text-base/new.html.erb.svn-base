<h1>New Goal Template</h1>

<% form_for(@goaltemplate) do |f| %>
  <%= f.error_messages %>

  <p>
    Title: <%= f.text_field :title, :size => 100 %>
  </p>
  <p>

    <%
    @arr = arr_goal_categories  
    %>
    Category: <%= f.select(:category, @arr)%>

  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', goaltemplates_path %>