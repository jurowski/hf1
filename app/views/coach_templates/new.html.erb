<h1>New coach_template</h1>

<% form_for(@coach_template) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :coach_user_id %><br />
    <%= f.text_field :coach_user_id %>
  </p>
  <p>
    <%= f.label :template_goal_id %><br />
    <%= f.text_field :template_goal_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', coach_templates_path %>