/*Form based CSS starts*/

.blog /* This Class controls External setting of Forms*/ {
	font-family:Verdana, Arial, Helvetica, sans-serif;
	color:#3B5966;
	font-size:11px;
	line-height: 18px;
}

.blog .form-head /* This Class controls heading */ {
	/*background-color: #263c46;
	border:1px solid #3a6f86;*/
	font-size:18px;
	font-weight:bold;
	color: #842321;
	padding:5px 0 16px 0px;
}
.blog .form-subhead /* This Class controls Sub heading */ {
	font-family:Trebuchet MS, Arial, Helvetica, sans-serif;
	font-size:20px;
	font-weight:bold;
	color: #281502;
	padding:0 0 5px 0px;
	margin:0px;
}
.blog .form-comments /* This Class controls Sub heading */ {
	background-color: #eae7db;
	font-family:Trebuchet MS, Arial, Helvetica, sans-serif;
	padding:2px 0px 0px 0px;
	color:#281502;
	font-size:11px;
}
.blog .form-comments p{
	font-family:Trebuchet MS, Arial, Helvetica, sans-serif;
	padding:3px 0px 0px 0px;
	color:#281502;
	font-size:11px;
}

.AncFormComment {
	font-family:Trebuchet MS, Arial, Helvetica, sans-serif;
	padding:0px 0px 0px 0px;
	margin:0px 0px 0px 0px;
	color:#281502;
	font-size:11px;
	vertical-align:top;
}


.blog .form-comments1 /* This Class controls Sub heading */ {
	background-color: #eae7db;
	font-family:Trebuchet MS, Arial, Helvetica, sans-serif;
	padding:4px 3px 4px 8px;
	color:#783f2f;
	font-size:13px;
	font-weight:bold;
}
.blog .OfbCustGridRow
{
	font-family:arial,Tahoma, Verdana, Arial;
	font-size:11px;
	padding: 1px;
	background-color:#FFFFFF;
}

.blog .OfbCustGridAlternateRow
{
	font-family:Arial, Helvetica, sans-serif;
	font-size:11px;
	color:#454F54;
	padding:1px;
	background-color:#ECEEF0;
}
.blog .expand{
	width:100%;
	height:1px;
}
.blog INPUT, .blog Select, .blog textarea /* This Class controls Select , textarea, INPUT */ {
	font-family:Verdana, Arial, Helvetica, sans-serif;
	color:#3C637D;
	font-size:10px;
	padding: 1px;	
	background-position:left top;
	border:1px solid #CFD3B5;
	background-color:#FDFEF5;
	vertical-align:middle;
	width:auto !important;
}
.blog .mandatory, .blog span.mandatory /* This Class controls Select , textarea, INPUT */ {
	color:#ED3800;
}
.blog INPUT.blog-btn /* This Class controls button*/ {
	background-image:none;
	background-repeat:repeat;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	color:#ffffff;
	background-color:#232f34;
	font-weight:bold;
	border:1px solid #3a6f86;
	margin:0px;
	padding:0px 1px;
}

/*Form based CSS End*/
